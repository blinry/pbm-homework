          RowBox[{
           RowBox[{"2", "cm"}], "-", 
           RowBox[{"4", "cm"}]}], "\[RightBracketingBar]"}]}], ")"}], "*", 
       FractionBox[
        RowBox[{
         RowBox[{"2", "cm"}], "-", 
         RowBox[{"4", "cm"}]}], 
        RowBox[{"\[LeftBracketingBar]", 
         RowBox[{
          RowBox[{"2", "cm"}], "-", 
          RowBox[{"4", "cm"}]}], "\[RightBracketingBar]"}]]}], "=", 
      RowBox[{
       RowBox[{"10", 
        FractionBox["g", 
         SuperscriptBox["s", "2"]], 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", "1"}], "cm"}], ")"}], "*", 
        RowBox[{"(", 
         RowBox[{"-", "1"}], ")"}]}], "=", 
       RowBox[{"10", "cm", " ", 
